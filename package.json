{
  "name": "most-cli",
  "version": "1.0.1",
  "description": "CLI utilities for working with Web Components and Polymer",
  "keywords": [
    "CLI",
    "node",
    "polymer",
    "web-components"
  ],
  "author": "Kus CÃ¡mara <kus.camara@gmail.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/kcmr/most.git"
  },
  "license": "MIT",
  "bin": {
    "most": "bin/cli.js"
  },
  "dependencies": {
    "chalk": "^2.1.0",
    "find-up": "^2.1.0",
    "markdown-table": "^1.1.1",
    "polymer-analyzer": "^2.3.0",
    "yargs": "^9.0.1"
  },
  "devDependencies": {
    "eslint": "^4.8.0",
    "eslint-config-xo-space": "^0.16.0",
    "jest": "^21.2.1",
    "pre-commit": "^1.2.2"
  },
  "scripts": {
    "pretest": "eslint . --fix",
    "test": "jest --coverage"
  },
  "pre-commit": [
    "pretest"
  ],
  "jest": {
    "testEnvironment": "node",
    "coverageDirectory": "./coverage/"
  },
  "eslintConfig": {
    "extends": "xo-space",
    "rules": {
      "capitalized-comments": 0,
      "no-warning-comments": 0,
      "radix": 0,
      "arrow-parens": [
        2,
        "as-needed",
        {
          "requireForBlockBody": true
        }
      ]
    },
    "env": {
      "jest": true,
      "node": true
    }
  },
  "eslintIgnore": [
    "coverage/*"
  ]
}
